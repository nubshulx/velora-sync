# ============================================================================
# VELORA SYNC CONFIGURATION
# ============================================================================

# REQUIRED: DOCUMENT PATHS
SOURCE_DOCUMENT_PATH=
DESTINATION_DOCUMENT_PATH=  

GOOGLE_SERVICE_ACCOUNT_FILE=

# REQUIRED: LLM CONFIGURATION
LLM_PROVIDER=
LLM_MODEL=
API_TOKEN=

# OPTIONAL: PROCESSING SETTINGS ('intelligent' (recommended), 'new_only', or 'full_sync')
UPDATE_MODE=

# Log level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=

# Maximum tokens for LLM generation (higher = more detailed test cases)
MAX_TOKENS=

# Temperature for generation (0.0-1.0, lower = more deterministic)
TEMPERATURE=

# OPTIONAL: UPSTASH REDIS CACHE CONFIGURATION
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# OPTIONAL: ADVANCED SETTINGS (defaults are usually fine)
REPORTS_DIR=./reports
CACHE_DIR=./cache
BATCH_SIZE=2
CREATE_BACKUP=true
MAX_RETRIES=3
API_TIMEOUT=300